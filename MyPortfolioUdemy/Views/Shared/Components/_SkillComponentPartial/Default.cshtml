@model List<MyPortfolioUdemy.DAL.Entities.Skill>
@{
	string deger = null;
}
<div class="col-six tab-full right">
	<h3>Programlama Dilleri & Teknolojiler</h3>

	<ul class="skill-bars">
		@foreach (var item in Model)
		{

			deger = $"percent{item.Value}";

			<li>
				<div class="progress @deger"><span>@item.Value%</span></div>
				<strong>@item.Title</strong>
			</li>
		}
	</ul>
</div>